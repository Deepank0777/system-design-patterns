      "target": "ES6",
      "module": "commonjs",
      "removeComments": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noImplicitAny": false,
      "outDir": "./build"
    },
    "filesGlob": [
      "./**/*.ts"
    ],
